@model IEnumerable<ExploreCalifornia.Models.Photo>

<h1>Explorer's Gallery</h1>

<p>
    This is the Explorer's Gallery; A gallery of the finest photography of landmarks in California.
</p>

<p class="pager">
    <!-- check parameters-->
    @if (ViewBag.HasPreviousPage)
    {
        <a class="prev" href="@Url.Action("Index", "Gallery", new { page = ViewBag.PreviousPage })">
            &lt; Previous
        </a>
    }

    @if (ViewBag.HasNextPage)
    {
        <a class="next" href="@Url.Action("Index", "Gallery", new { page = ViewBag.NextPage })">
            Next &gt;
        </a>
    }
</p>

<div class="photos">
    @foreach (var photo in Model)
    {
        @Html.Partial("_GalleryPost", photo)
    }
</div>

